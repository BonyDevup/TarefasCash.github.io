{
  "start_url": "./index.html",
  "icons": [
    {
      "src": "./favicon.ico",
      "sizes": "64x64",
      "type": "image/x-icon"
    }
  ],
  "name": "TarefasCash",
  "short_name": "TarefasCash",
  "description": "Gerenciador de Tarefas",
